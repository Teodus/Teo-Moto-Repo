    "# Create route selection interface\n",
    "def create_route_selector():\n",
    "    \"\"\"Create a route selection widget with available GPX files.\"\"\"\n",
    "    \n",
    "    # Start with demo routes (always available)\n",
    "    available_routes = [\n",
    "        (\"Demo Route - Flat Terrain\", \"demo_flat\"),\n",
    "        (\"Demo Route - Hilly Terrain\", \"demo_hills\"),\n",
    "        (\"Demo Route - Mixed Terrain\", \"demo_mixed\")\n",
    "    ]\n",
    "    \n",
    "    # Try to add real GPX files if available\n",
    "    gpx_dir = \"../docs/gpx_files\"\n",
    "    if os.path.exists(gpx_dir):\n",
    "        for file in os.listdir(gpx_dir):\n",
    "            if file.endswith('.gpx'):\n",
    "                # Create friendly names for routes\n",
    "                route_name = file.replace('.gpx', '').replace('-', ' ').replace('_', ' ').title()\n",
    "                available_routes.append((f\"GPS Track - {route_name}\", os.path.join(gpx_dir, file)))\n",
    "    \n",
    "    route_selector = widgets.Dropdown(\n",
    "        options=available_routes,\n",
    "        description='Test Route:',\n",
    "        style={'description_width': 'initial'},\n",
    "        layout=widgets.Layout(width='400px')\n",
    "    )\n",
    "    \n",
    "    # Route info display\n",
    "    route_info = widgets.HTML(\n",
    "        value=\"<p><i>Select a route to see details...</i></p>\",\n",
    "        layout=widgets.Layout(margin='10px 0')\n",
